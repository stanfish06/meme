	<pattern accession="+2" name="+2">
		<scanned-sequence accession="Scer" name="Scer" score="0.113502"
			pvalue="0.625522" length="2666">
		</scanned-sequence>
	</pattern>
	<pattern accession="+3" name="+3">
		<scanned-sequence accession="Scer" name="Scer" score="0.309739"
			pvalue="0.579501" length="2666">
		</scanned-sequence>
	</pattern>
</cis-element-search>